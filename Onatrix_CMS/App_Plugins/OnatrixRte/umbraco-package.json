{
  "name": "Onatrix RTE",
  "alias": "Onatrix.RTE",
  "extensions": [
    {
      "type": "tiptapToolbarExtension",
      "kind": "styleMenu",
      "alias": "Onatrix.Tiptap.BrandColors",
      "name": "Onatrix Brand Colors",
      "meta": {
        "alias": "onatrixBrandColors",
        "label": "Onatrix Colors"
      },
      "items": [
        {
          "label": "Primary",
          "data": {
            "tag": "span",
            "class": "text-primary"
          },
          "appearance": {
            "style": "background-color: var(--color-primary); color: var(--color-white-100); padding:5px;"
          }
        },
        {
          "label": "Secondary",
          "data": {
            "tag": "span",
            "class": "text-secondary"
          },
          "appearance": {
            "style": "background-color: var(--color-secondary); color: var(--color-text); padding:5px;"
          }
        },
        {
          "label": "Body",
          "data": {
            "tag": "span",
            "class": "text-body-text"
          },
          "appearance": {
            "style": "background-color: var(--color-text); color: var(--color-white-100); padding:5px;"
          }
        },
        {
          "label": "Body-Light",
          "data": {
            "tag": "span",
            "class": "text-body-text-100"
          },
          "appearance": {
            "style": "background-color: var(--color-note); color: var(--color-white-100); padding:5px;"
          }
        }
      ]
    },
